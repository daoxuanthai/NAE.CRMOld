@using tcs.lib
@using tcs.bo
@model List<ContractProcessBo>
@if (Model != null && Model.Any())
{
    var index = 1;
    <table class="table table-striped table-bordered dTableR popuptbl">
        <thead>
            <tr>
                <th class="center w80">STT</th>
                <th class="center">Nội dung cập nhật</th>
                <th class="center">Người cập nhật</th>
                <th class="center">Ngày tạo</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Any())
            {
                foreach (var item in Model)
                {
                    <tr id="commission_@item.Id">
                        <td class="center">@(index++)</td>
                        <td class="rel">
                            @item.ProcessNote
                        </td>
                        <td class="center">
                            @item.CreateUserName
                        </td>
                        <td class="center">
                            @item.CreateDate.Value.ToDateTimeString()
                        </td>
                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="4" class="center">Chưa có dữ liệu</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="clr"></div>
}